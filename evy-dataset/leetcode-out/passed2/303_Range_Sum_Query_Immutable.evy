
func NumArray:{}any nums:[]num
    self:{}any
    self.res := [0] * (len nums)
    self.data := nums[:]
    for i := range (len self.data)
        self.res[i] = self.res[i - 1] + nums[i]
    end
    return self
end

func sumRange:num self:{}any i:num j:num
    return self.res[j] - self.res[i - 1]
end

func test
    nums := [1 2 3 4 5]
    numArray := (NumArray nums)
    assert 3 (sumRange numArray 1 2)
    assert 5 (sumRange numArray 2 3)
end
test
