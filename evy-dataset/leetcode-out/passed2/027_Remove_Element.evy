
func removeElement:[]num nums:[]num val:num
    ls := len nums
    if ls == 0
        return nums
    end
    count := 0
    i := 0
    while i < ls - count
        if nums[i] == val
            nums[i] = nums[ls - 1 - count]
            count = count + 1
        else
            i = i + 1
        end
    end
    return nums[:ls - count]
end

func test
    assert [] (removeElement [] 1)
    assert [1] (removeElement [1] 1)
    assert [2 2] (removeElement [3 2 2 3] 3)
    assert [0 1 3 0 4] (removeElement [0 1 2 2 3 0 4 2] 2)
end
test
