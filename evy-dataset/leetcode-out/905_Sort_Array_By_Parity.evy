
func sortArrayByParity:[]num A:[]num
    lo := 0
    hi := (len A) - 1
    while lo < hi
        if (A[lo] % 2) > (A[hi] % 2)
            A[lo], A[hi] = A[hi], A[lo]
        end
        if (A[lo] % 2) == 0
            lo = lo + 1
        end
        if (A[hi] % 2) == 1
            hi = hi - 1
        end
    end
    return A
end

func test
    assert [2 4 6 1 3 5] (sortArrayByParity [1 4 2 5 3 6])
    assert [2 4 6 1 3 5] (sortArrayByParity [4 2 6 1 3 5])
    assert [2 4 6 1 3 5] (sortArrayByParity [6 4 2 1 3 5])
    assert [2 4 6 1 3 5] (sortArrayByParity [5 3 1 6 4 2])
    assert [2 4 6 1 3 5] (sortArrayByParity [3 1 5 2 4 6])
    assert [2 4 6 1 3 5] (sortArrayByParity [1 2 3 4 5 6])
end
test
