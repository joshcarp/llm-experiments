
func removeElement:[]num nums:[]num val:num
    ls := len nums
    if ls == 0
        return ls
    end
    count := 0
    index := 0
    while index < ls - count
        if nums[index] == val
            nums[index] = nums[ls - 1 - count]
            count = count + 1
        else
            index = index + 1
        end
    end
    return ls - count
end

func test
    s := {}
    assert 0 (removeElement [] 1)
    assert 0 (removeElement [1] 1)
    assert 2 (removeElement [3 2 2 3] 3)
    assert 5 (removeElement [0 1 2 2 3 0 4 2] 2)
end
test
