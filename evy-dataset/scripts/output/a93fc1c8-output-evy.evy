
func sublists:num min:num length:num nums:[]num
    result := 0
    for start := range (len nums)
        for end := range start+length (len nums)
            if nums[end] >= min
                result = result + 1
            end
        end
    end
    return result
end

print (sublists 2 3 [1 2 3 4 5])
     
