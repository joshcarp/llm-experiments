
func ListNode:{}any
    self:{}any
    self.val := 0
    self.next := {}
    return self
end

func Solution:{}any
    self:{}any
    return self
end

func swapPairs self:{}any head:{}any:any
    dummyHead := (ListNode)
    dummyHead.val = -1
    dummyHead.next = head
    prev:{}any
    p:{}any
    prev = dummyHead
    p = head
    while p != {} and p.next != {}
        q:{}any
        r:{}any
        q = p.next
        r = p.next.next
        prev.next = q
        q.next = p
        p.next = r
        prev = p
        p = r
    end
    return dummyHead.next
end

func test
    head := (ListNode)
    head.val = 1
    head.next = (ListNode)
    head.next.val = 2
    head.next.next = (ListNode)
    head.next.next.val = 3
    head.next.next.next = (ListNode)
    head.next.next.next.val = 4
    sol := (Solution)
    newHead := sol.swapPairs head
    print newHead.val // Prints: 2
    print newHead.next.val // Prints: 1
    print newHead.next.next.val // Prints: 3
    print newHead.next.next.next.val // Prints: 4
end
