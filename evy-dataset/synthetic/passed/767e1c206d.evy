//Implement a stack using an array

func Stack:{}any
    self:{}any
    self.arr := []
    self.top := -1
    return self
end

func push self:{}any val:any
    self.top = self.top + 1
    self.arr = self.arr + [val]
end

func pop self:{}any:any
    if self.top == -1
        return -1
    end
    val := self.arr[self.top]
    self.arr = self.arr[:-1]
    self.top = self.top - 1
    return val
end

func peek self:{}any:any
    if self.top == -1
        return -1
    end
    return self.arr[self.top]
end

func isEmpty self:{}any:bool
    return self.top == -1
end

    stack := (Stack)
    push stack 10
    push stack 20
    push stack 30
    assert 30 (peek stack)
    assert 30 (pop stack)
    assert 20 (peek stack)
    assert 20 (pop stack)
    assert 10 (peek stack)
    assert 10 (pop stack)
    assert true (isEmpty stack)

