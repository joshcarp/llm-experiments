
func isAnagram:bool s:string t:string
    if (len s) != (len t)
        return false
    end
    counter := [0] * 26
    for i := range (len s)
        counter[ord(s[i]) - ord("a")] = counter[ord(s[i]) - ord("a")] + 1
        counter[ord(t[i]) - ord("a")] = counter[ord(t[i]) - ord("a")] - 1
    end
    for num := range counter
        if num != 0
            return false
        end
    end
    return true
end

func test
    assert true==(isAnagram "anagram" "nagaram")
    assert false==(isAnagram "rat" "car")
end
test
