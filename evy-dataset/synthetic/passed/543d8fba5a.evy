//Generate binary numbers from 1 to n using a queue

func generate_binary_numbers:[]string n:num
    queue := ["1"]
    result := []
    for i := range n
        current := queue[0]
        queue = queue[1:]
        result = result + [current]
        queue = queue + [current + "0"] + [current + "1"]
    end
    return result
end

assert ["1" "10" "11" "100" "101" "110" "111"] (generate_binary_numbers 7)
