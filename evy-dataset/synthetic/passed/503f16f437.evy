//Determine if an array is sorted in ascending order

func isSorted:bool arr:[]num
    for i := range (len arr) - 1
        if i > 0 and arr[i] < arr[i - 1]
            return false
        end
    end
    return true
end

    assert true (isSorted [1 2 3 4 5])
    assert false (isSorted [1 3 2 4 5])
    assert true (isSorted [1 1 1 1 1])
    assert true (isSorted [])

