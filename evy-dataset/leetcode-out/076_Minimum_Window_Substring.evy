
func minWindow:string s:string t:string
    lsS := len s
    lsT := len t
    needToFind := [0] * 256
    hasFound := [0] * 256
    minBegin := 0
    minEnd := -1
    minWindow := 100000000000000
    for index := range lsT
        needToFind[ord t[index]] = needToFind[ord t[index]] + 1
    end
    count := 0
    begin := 0
    for end := range lsS
        endIndex := ord s[end]
        if needToFind[endIndex] == 0
            continue
        end
        hasFound[endIndex] = hasFound[endIndex] + 1
        if hasFound[endIndex] <= needToFind[endIndex]
            count = count + 1
        end
        if count == lsT
            beginIndex := ord s[begin]
            while needToFind[beginIndex] == 0 or hasFound[beginIndex] > needToFind[beginIndex]
                if hasFound[beginIndex] > needToFind[beginIndex]
                    hasFound[beginIndex] = hasFound[beginIndex] - 1
                end
                begin = begin + 1
                beginIndex = ord s[begin]
            end
            windowLs := end - begin + 1
            if windowLs < minWindow
                minBegin = begin
                minEnd = end
                minWindow = windowLs
            end
        end
    end
    if count == lsT
        return s[minBegin:minEnd + 1]
    else
        return ""
    end
end

func test
    s := Solution
    print (minWindow s "a" "a")
end
test

func Solution:{}any
    self:{}any
    return self
end
