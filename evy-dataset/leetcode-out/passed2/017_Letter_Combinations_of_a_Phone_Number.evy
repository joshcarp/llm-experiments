
dmap := {"2":"abc" "3":"def" "4":"ghi" "5":"jkl" "6":"mno" "7":"pqrs" "8":"tuv" "9":"wxyz" "0":" "}

func letterCombinations:string digits:string
    result := []string
    ls := len digits
    if ls == 0
        return ""
    end
    current := digits[0]
    posfix := letterCombinations digits[1:]
    for t := range dmap[current]
        if (len posfix) > 0
            for p := range posfix
                temp := t + p
                result = result + [temp]
            end
        else
            result = result + [t]
        end
    end
    return join result " "
end

func test
    assert "" (letterCombinations "")
    assert "ad" (letterCombinations "23")
    assert "ad ae af bd be bf cd ce cf" (letterCombinations "223")
end
test
