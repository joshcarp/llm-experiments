
func defangIPaddr:string address:string
    return replace address "." "[.]"
end

func test
    assert "[.]1[.]1[.]1[.]1" (defangIPaddr "1.1.1.1")
    assert "[.]255[.]100[.]1[.]1" (defangIPaddr "255.100.1.1")
end
