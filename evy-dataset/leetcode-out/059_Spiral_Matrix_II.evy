
func generateMatrix:[][]num n:num
    res := [[]]
    for i := range n
        res = res + [[0] * n]
    end
    pos := [0 0]
    move := [0 1]
    for index := range 1 (n * n + 1)
        res[pos[0]][pos[1]] = index
        if res[(pos[0] + move[0]) % n][(pos[1] + move[1]) % n] > 0
            // (0, 1) -> (1, 0) -> (0, -1) -> (-1, 0)
            move = [move[1] (-1 * move[0])]
        end
        pos[0] = pos[0] + move[0]
        pos[1] = pos[1] + move[1]
    end
    return res
end

func test
    print (generateMatrix 2)
end
test
