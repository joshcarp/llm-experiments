//Implement depth-first search (DFS)

func dfs:bool graph:[][]num start:num end:num visited:{}bool
    visited[start] = true
    if start == end
        return true
    end
    for neighbor := range graph[start]
        if !(has visited neighbor)
            if dfs graph neighbor end visited
                return true
            end
        end
    end
    return false
end

    graph := [[1 2] [2] [0 3] [3]]
    start := 0
    end := 3
    visited := {}
    assert true (dfs graph start end visited)

