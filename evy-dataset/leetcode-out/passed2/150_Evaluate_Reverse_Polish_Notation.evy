
func evalRPN:num tokens:[]string
    stack := []num
    for t := range tokens
        if t == "+" or t == "-" or t == "*" or t == "/"
            b := stack[-1]
            stack = stack[:-1]
            a := stack[-1]
            stack = stack[:-1]
            if t == "+"
                a = a + b
            else if t == "-"
                a = a - b
            else if t == "*"
                a = a * b
            else
                a = floor (a / b)
            end
            stack = stack + [a]
        else
            stack = stack + [str2num t]
        end
    end
    return stack[-1]
end

func test
    assert 9 (evalRPN ["2" "1" "+" "3" "*"])
    assert 6 (evalRPN ["4" "13" "5" "/" "+"])
    assert 22 (evalRPN ["10" "6" "9" "3" "+" "-" "11" "*" "17" "+" "5" "+"])
end
test
