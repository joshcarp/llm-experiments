
func TreeNode:{}any
    self:{}any
    self.val = 0
    self.left = {}
    self.right = {}
    return self
end

func maxDepth:num root:{}any
    if root == {}
        return 0
    end
    return 1 + (max (maxDepth root.left) (maxDepth root.right))
end

func test
    root := (TreeNode)
    root.val = 3
    root.left = (TreeNode)
    root.left.val = 9
    root.right = (TreeNode)
    root.right.val = 20
    root.right.left = (TreeNode)
    root.right.left.val = 15
    root.right.right = (TreeNode)
    root.right.right.val = 7
    assert 3 (maxDepth root)
end
test
