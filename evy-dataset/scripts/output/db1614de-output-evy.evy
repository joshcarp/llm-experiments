func subarray_count:num arr:[]num length:num
    if length > (len arr)
        return 0
    end
    count := 0
    for i := range length (len arr)
        count = count + 1
    end
    return count
end

numbers := [1 2 3 4 5]
length := 3
count := subarray_count numbers length
print count // Prints: 3

     
