//Find the majority element in an array (appears more than n/2 times)

func majorityElement:num nums:[]num
    counts := {}num
    for num := range nums
        if has counts (sprint num)
            counts[sprint num] = counts[sprint num] + 1
        else
            counts[sprint num] = 1
        end
    end
    for key := range counts
        if counts[key] > (len nums) / 2
            return str2num key
        end
    end
    return -1
end

    assert 2 (majorityElement [2 2 1 1 1 2 2])
    assert 3 (majorityElement [3 2 3])
    assert 2 (majorityElement [2 2 1 1 1 2 2])

