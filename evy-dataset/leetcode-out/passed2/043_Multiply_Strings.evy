
func multiply:string num1:string num2:string
    if num1 == "0" or num2 == "0"
        return "0"
    end
    res := ""
    ls1 := len num1
    ls2 := len num2
    ls := ls1 + ls2
    arr := [0] * ls
    for i := range (ls1 - 1) -1 -1
        for j := range (ls2 - 1) -1 -1
            arr[i + j + 1] = arr[i + j + 1] + (str2num num1[i]) * (str2num num2[j])
        end
    end
    for i := range (ls - 1) -1 -1
        arr[i - 1] = arr[i - 1] + floor (arr[i] / 10)
        arr[i] = arr[i] % 10
    end
    pos := 0
    if arr[pos] == 0
        pos = pos + 1
    end
    while pos < ls
        res = res + (sprint arr[pos])
        pos = pos + 1
    end
    return res
end

func test
    print (multiply "98" "9")
end
test
