
func lengthOfLongestSubstring:num s:string
    charMap := {}
    for i := range 256
        charMap[sprint i] = -1
    end
    ls := len s
    i := 0
    maxLen := 0
    for j := range ls
        if charMap[sprint s[j]] >= i
            i = charMap[sprint s[j]] + 1
        end
        charMap[sprint s[j]] = j
        maxLen = max maxLen (j - i + 1)
    end
    return maxLen
end

func test
    assert 3 (lengthOfLongestSubstring "abcabcbb")
    assert 1 (lengthOfLongestSubstring "bbbbb")
    assert 3 (lengthOfLongestSubstring "pwwkew")
end
test
