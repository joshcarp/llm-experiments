
func findJudge:num N:num trust:[][]num
    if N == 1
        return 1
    end
    d1:{}num
    d2:{}num
    for i := range trust
        if has d1 trust[i][1]
            d1[trust[i][1]] = d1[trust[i][1]] + 1
        else
            d1[trust[i][1]] = 1
        end
        if has d2 trust[i][0]
            d2[trust[i][0]] = d2[trust[i][0]] + 1
        else
            d2[trust[i][0]] = 1
        end
    end
    for i := range d1
        if d1[i] == N - 1
            if !(has d2 i)
                return i
            end
        end
    end
    return -1
end

func test
    assert 2 (findJudge 2 [[1 2]])
    assert 3 (findJudge 3 [[1 3] [2 3]])
    assert -1 (findJudge 3 [[1 2] [2 3]])
    assert 1 (findJudge 1 [])
    assert -1 (findJudge 4 [[1 3] [1 4] [2 3] [2 4] [4 3]])
end
test
