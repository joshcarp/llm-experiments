
func intersection:Node* head1:Node* head2:Node*
    p1 := head1
    p2 := head2
    while p1 != p2
        if p1 == nil
            p1 = head2
        else
            p1 = p1.next
        end
        if p2 == nil
            p2 = head1
        else
            p2 = p2.next
        end
    end
    return p1
end

