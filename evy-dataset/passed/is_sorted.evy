
func isSorted:bool node:{}any
    if (has node "left") and (node["left"].({}any) != {} and (node["data"] < node["left"]["data"].(num) or !(isSorted node["left"])))
        return false
    end
    if (has node "right") and (node["right"].({}any) != {} and (node["data"] > node["right"]["data"].(num) or !(isSorted node["right"])))
        return false
    end
    return true
end

func test
    node := {data:2.1 left:{} right:{}}
    assert true==(isSorted node)
    node["left"] = {data:2.0 left:{} right:{}}
    assert true==(isSorted node)
    node["right"] = {data:2.2 left:{} right:{}}
    assert true==(isSorted node)
    node["right"]["data"] = 2.0
    assert false==(isSorted node)
    node["right"]["data"] = 2.1
    assert true==(isSorted node)
end
test
