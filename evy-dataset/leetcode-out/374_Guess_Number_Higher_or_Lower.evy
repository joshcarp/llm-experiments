func guessNumber:num n:num
    begin := 1
    end := n
    while begin <= end
        mid := floor (begin + end)/2
        res := guess mid
        if res == 0
            return mid
        else if res > 0
            begin = mid + 1
        else
            end = mid - 1
        end
    end
    return -1 // Should never reach here
end

