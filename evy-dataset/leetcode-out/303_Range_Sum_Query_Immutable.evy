
func NumArray:{}any nums:[]num
    self:{}any
    self.res := [0] * (len nums + 1)
    self.data := nums[:]
    for i := range (len self.data)
        self.res[i + 1] = self.res[i] + nums[i]
    end
    return self
end

func sumRange:num self:{}any i:num j:num
    return self.res[j + 1] - self.res[i]
end

func test
    nums := [1 2 3 4 5]
    numArray := (NumArray nums)
    assert 3 (sumRange numArray 0 1)
    assert 5 (sumRange numArray 1 2)
end
test
