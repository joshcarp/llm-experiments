
func productExceptSelf:[]num nums:[]num
    ans := [1] * (len nums)
    for i := range 1 (len nums)
        ans[i] = ans[i - 1] * nums[i - 1]
    end
    right := 1
    for i := range (len nums) - 1 -1 -1
        ans[i] = ans[i] * right
        right = right * nums[i]
    end
    return ans
end

func test
    assert [24 12 8 6] (productExceptSelf [1 2 3 4])
    assert [0 0 9 0 0] (productExceptSelf [0 1 2 3 0])
end
