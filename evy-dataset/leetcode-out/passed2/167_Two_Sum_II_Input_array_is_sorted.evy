
func twoSum:[]num numbers:[]num target:num
    begin := 0
    end := (len numbers) - 1
    while begin < end
        curr := numbers[begin] + numbers[end]
        if curr == target
            return [begin + 1 end + 1]
        else if curr < target
            begin = begin + 1
        else
            end = end - 1
        end
    end
    return []
end

func test
    assert [1 2] (twoSum [2 7 11 15] 9)
    assert [1 2] (twoSum [2 3 4] 6)
    assert [1 2] (twoSum [2 3 4] 5)
end
