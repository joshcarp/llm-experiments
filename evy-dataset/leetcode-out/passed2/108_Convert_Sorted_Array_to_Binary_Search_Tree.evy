
func TreeNode:{}any
    self:{}any
    self.val = 0
    self.left = {}
    self.right = {}
    return self
end

func Solution:{}any
    self:{}any
    return self
end

func sortedArrayToBST self:{}any nums:[]num
    return self.getHelper nums 0 (len nums) - 1
end

func getHelper self:{}any nums:[]num start:num end:num
    if start > end
        return {}
    end
    mid := (start + end) / 2
    node := (TreeNode)
    node.val = nums[mid]
    node.left = self.getHelper nums start (mid - 1)
    node.right = self.getHelper nums (mid + 1) end
    return node
end

func test
    s := (Solution)
    nums := [1 2 3 4 5 6 7]
    root := sortedArrayToBST s nums
    print root
end
test
