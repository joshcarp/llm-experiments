//Find all factors of a number

func factors:[]num n:num
    result := []
    for i := range 1 (n + 1)
        if n % i == 0
            result = result + [i]
        end
    end
    return result
end

    assert [1 2 3 4 6 12] (factors 12)
    assert [1 2 4 8 16 32 64 128 256 512 1024 2048] (factors 2048)
    assert [1 2 3 6 9 18 27 54 81 162 243 486] (factors 486)

