
func flipAndInvertImage:[][]num A:[][]num
    for row := range A
        for i := range (floor (len row) / 2)
            row[i] = 1 - row[i]
            row[len row - i - 1] = 1 - row[len row - i - 1]
            tmp := row[i]
            row[i] = row[len row - i - 1]
            row[len row - i - 1] = tmp
        end
        if (len row) % 2 == 1
            row[floor (len row) / 2] = 1 - row[floor (len row) / 2]
        end
    end
    return A
end

func test
    assert [[1 1 0] [0 1 1] [0 0 0]] (flipAndInvertImage [[1 0 0] [0 1 0] [1 1 1]])
    assert [[1 0] [0 1]] (flipAndInvertImage [[1 1] [0 0]])
end
test
