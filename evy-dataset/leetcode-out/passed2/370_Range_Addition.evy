
func getModifiedArray:[]num length:num updates:[][]num
    res := [0] * length
    for i := range updates
        start := updates[i][0]
        end := updates[i][1]
        val := updates[i][2]
        res[start] = res[start] + val
        if end < length - 1
            res[end + 1] = res[end + 1] - val
        end
    end
    for i := range 1 length
        res[i] = res[i] + res[i - 1]
    end
    return res
end

func test
    assert [1 1 1 1 1] (getModifiedArray 5 [[1 3 2]])
    assert [1 2 3 4 5] (getModifiedArray 5 [[1 3 2] [2 4 3] [0 2 -2]])
end
test
