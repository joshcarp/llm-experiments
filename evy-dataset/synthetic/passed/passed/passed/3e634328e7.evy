//Remove all spaces from a string

func removeSpaces:string str:string
    result := ""
    for ch := range str
        if ch != " "
            result = result + ch
        end
    end
    return result
end

func test
    assert "HelloWorld" (removeSpaces "Hello World")
    assert "Evy" (removeSpaces "Evy")
    assert "" (removeSpaces " ")
end
