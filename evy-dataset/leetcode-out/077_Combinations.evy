
func combine:[][]num self:{}any n:num k:num
    res:[][]num
    self.getCombine res [] n k 1
    return res
end

func getCombine:void self:{}any res:[][]num prefix:[]num n:num k:num start:num
    if k == 0
        res = res + [prefix[:]]
    else if start <= n
        prefix = prefix + [start]
        self.getCombine res prefix n (k - 1) (start + 1)
        prefix = prefix[:-1]
        self.getCombine res prefix n k (start + 1)
    end
end

func test
    s := {}
    print (combine s 4 2)
end
