//Merge two sorted arrays into one sorted array

func merge nums1:[]num m:num nums2:[]num n:num
    p1 := m - 1
    p2 := n - 1
    pos := m + n - 1
    while p1 >= 0 and p2 >= 0
        if nums1[p1] >= nums2[p2]
            nums1[pos] = nums1[p1]
            p1 = p1 - 1
        else
            nums1[pos] = nums2[p2]
            p2 = p2 - 1
        end
        pos = pos - 1
    end
    while p2 >= 0
        nums1[pos] = nums2[p2]
        p2 = p2 - 1
        pos = pos - 1
    end
end

nums1 := [1 2 3 0 0 0]
m := 3
nums2 := [2 5 6]
n := 3
merge nums1 m nums2 n
assert [1 2 2 3 5 6] nums1

